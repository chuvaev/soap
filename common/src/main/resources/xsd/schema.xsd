<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<xs:definitions
        xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
        xmlns:s="http://www.w3.org/2001/XMLSchema" name="Contacts"
        targetNamespace="http://webservices.ilyachuvaev.com/Contacts/"
        xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
        xmlns:xs="http://schemas.xmlsoap.org/wsdl/"
        xmlns:contacts="http://com/ilyachuvaev/webservices/contactservice"
        xmlns:tns="http://webservices.ilyachuvaev.com/Contacts/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >

    <xs:documentation xmlns:xs="http://schemas.xmlsoap.org/wsdl/"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://schemas.xmlsoap.org/wsdl/ ">
        Contact Service
    </xs:documentation>

    <xs:types>
        <s:import namespace="http://com/ilyachuvaev/webservices/contactservice"
                  schemaLocation="contacts.xsd">
        </s:import>
    </xs:types>

    <xs:message name="ContactDetailsRequest">
        <xs:part element="contacts:ContactDetailsRequest" name="parameters"/>
    </xs:message>
    <xs:message name="ContactDetailsResponse">
        <xs:part element="contacts:ContactDetailsResponse" name="parameters"/>
    </xs:message>

    <xs:portType name="Contacts">
        <xs:operation name="GetContactDetails">
            <xs:input message="tns:ContactDetailsRequest"/>
            <xs:output message="tns:ContactDetailsResponse"/>
        </xs:operation>
    </xs:portType>

    <xs:binding name="ContactServiceSoapBinding" type="tns:Contacts">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <xs:operation name="GetContactDetails">
            <soap:operation soapAction="http://www.ilyachuvaev.com/Contacts/GetContactDetails"/>
            <xs:input>
                <soap:body use="literal"/>
            </xs:input>
            <xs:output>
                <soap:body use="literal"/>
            </xs:output>
        </xs:operation>
    </xs:binding>

    <xs:service name="ContactService">
        <xs:port binding="tns:ContactServiceSoapBinding" name="ContactsPort">
            <soap:address location="http://localhost:8080/services/contacts"/>
        </xs:port>
    </xs:service>
</xs:definitions>